{
  "version": 1,
  "projects": {
    "login": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/login",
      "sourceRoot": "apps/login/src",
      "prefix": "facehiring",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/login",
            "index": "apps/login/src/index.html",
            "main": "apps/login/src/main.ts",
            "polyfills": "apps/login/src/polyfills.ts",
            "tsConfig": "apps/login/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/login/src/favicon.ico",
              "apps/login/src/assets"
            ],
            "styles": [
              "apps/login/src/styles.css"
            ],
            "scripts": [
              "apps/login/src/assets/js/jquery-1.11.1.min.js",
              "apps/login/src/assets/js/jquery-ui-min.js",
              "apps/login/src/assets/js/svg-loader.js"
              
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "./environments/environment.ts",
                  "with": "./environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "login:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "login:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "login:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/login/tsconfig.app.json",
              "apps/login/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/login/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/login/jest.config.js",
            "tsConfig": "apps/login/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/login/src/test-setup.ts"
          }
        }
      }
    },
    "login-e2e": {
      "root": "apps/login-e2e",
      "sourceRoot": "apps/login-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/login-e2e/cypress.json",
            "tsConfig": "apps/login-e2e/tsconfig.e2e.json",
            "devServerTarget": "login:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "login:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/login-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/login-e2e/**/*"
            ]
          }
        }
      }
    },
